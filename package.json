{
    "name": "git-stash-remote",
    "description": "Easily transfer stashes through the git remote.",
    "version": "0.0.0",
    "author": "Ian Hornik",
    "bin": "./bin/run.js",
    "bugs": "https://github.com/yiliansource/git-stash-remote/issues",
    "dependencies": {
        "@inquirer/prompts": "^7.4.0",
        "@oclif/core": "^4",
        "@oclif/plugin-help": "^6",
        "@oclif/plugin-plugins": "^5",
        "date-fns": "^4.1.0",
        "nano-spawn": "^0.2.0",
        "ora": "^8.2.0",
        "terminal-link": "^3.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1",
        "@oclif/prettier-config": "^0.2.1",
        "@oclif/test": "^4",
        "@types/chai": "^4",
        "@types/mocha": "^10",
        "@types/node": "^18",
        "chai": "^4",
        "eslint": "^9",
        "eslint-config-oclif": "^6",
        "eslint-config-prettier": "^10",
        "mocha": "^10",
        "oclif": "^4",
        "prettier": "^3.5.3",
        "shx": "^0.3.3",
        "ts-node": "^10",
        "typescript": "^5"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "./bin",
        "./dist",
        "./oclif.manifest.json"
    ],
    "homepage": "https://github.com/yiliansource/git-stash-remote",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "type": "module",
    "oclif": {
        "bin": "git-stash-remote",
        "dirname": "git-stash-remote",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help"
        ],
        "hooks": {
            "prerun": "./dist/hooks/prerun"
        },
        "topicSeparator": " "
    },
    "repository": "yiliansource/git-stash-remote",
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "eslint",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "format": "prettier --write ."
    },
    "types": "dist/index.d.ts",
    "packageManager": "yarn@4.7.0"
}
